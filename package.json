{
  "name": "yunxiao-cli",
  "version": "0.1.1",
  "description": "yx - Yunxiao CLI powered by Yunxiao OpenAPI",
  "type": "module",
  "license": "Apache-2.0",
  "bin": {
    "yx": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "check:command-docs": "node scripts/check-command-docs.mjs",
    "check:command-ux": "node scripts/check-command-ux.mjs",
    "check:usage-examples": "node scripts/check-usage-examples.mjs",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "e2e:smoke": "node scripts/e2e-smoke.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "tsx": "^4.20.6",
    "typescript": "^5.8.2"
  }
}
